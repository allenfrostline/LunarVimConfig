{
    "Front matter": {
        "prefix": "---",
        "description": "Insert a simple front matter",
        "body": [
            "---",
            "title: ${1}",
            "category: ${2|Tech,Life|}",
            "date: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE",
            "---"
        ]
    },
    "Details": {
        "prefix": "details",
        "description": "Insert a collapsible details tag",
        "body": [
            "<details><summary>$1</summary>$2</details>"
        ]
    }
}
